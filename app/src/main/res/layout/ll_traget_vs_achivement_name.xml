<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/BLACK"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="@dimen/width_zero_dip"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_height="@dimen/height_50_dip"
                android:layout_width="@dimen/width_eighty_five_dip"
                android:background="@color/BLACK"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/ta_dealer_no"
                    android:layout_width="@dimen/width_eighty_five_dip"
                    android:paddingRight="@dimen/padding_four_dip"
                    android:layout_height="match_parent"
                    android:gravity="left|center_vertical"
                    android:background="@color/WHITE"
                    android:layout_marginLeft="@dimen/margin_point_one_dip"
                    android:layout_marginBottom="0.1dp"
                    android:text=""
                    android:textColor="@color/header_tile_text_color"
                    android:textSize="@dimen/medium_text"/>

            </LinearLayout>


        </LinearLayout>



    </LinearLayout>
    <!-- <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
         android:layout_width="match_parent"
         android:layout_height="1dp"
         android:background="@color/BLACK">

     </LinearLayout>-->



</LinearLayout>