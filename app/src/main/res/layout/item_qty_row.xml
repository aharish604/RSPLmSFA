<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@color/WHITE" >

    <TextView
        android:id="@+id/item_lable"
        android:layout_width="wrap_content"
        android:gravity="left"
        android:paddingLeft="@dimen/padding_three_dip"
        android:textColor="@color/BLACK"
        android:textSize="@dimen/fifteen_sp" />
    <TextView
        android:id="@+id/item_blank"
        android:layout_width="wrap_content"
        android:gravity="center"
        android:textColor="@color/BLACK"
        android:textSize="@dimen/fifteen_sp"
        android:width="@dimen/margin_ten_dip" />

    <TextView
        android:layout_weight="1"
        android:id="@+id/item_value"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/margin_five_dip"
        android:gravity="left"
        android:paddingLeft="@dimen/padding_two_dip"
        android:singleLine="false"
        android:textColor="@color/BLACK"
        android:textSize="@dimen/fifteen_sp" />

